{
  "name": "zoning-platform-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "npm run type-check",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "quality": "npm run type-check && npm run lint && npm run test:coverage",
    "quality:fix": "npm run lint:fix && npm run format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup:dev": "node dev-tools.js setup",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "axios": "^1.6.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lodash": "^4.17.23",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^2.15.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.202",
    "@types/mapbox-gl": "^2.7.0",
    "autoprefixer": "^10.4.0",
    "critters": "^0.0.23",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.5",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.3.6",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead"
  ]
}
